    var newPlot = function(dataInput, fillColor){
      dataset = dataInput

      svg.selectAll("circle")
        .data(dataset)
        .transition()
        .ease('linear')
        .delay(function(d, i) {
          return i * 10;
        })
        .attr('cx', function(d, i) {
          return xScale(i);
        })
        .attr('cy', function(d) {
          return yScale(d);
        })
        .attr('fill', fillColor)
    }

    var bestFitLine = function(train) {
      $('.best-fit').remove();
      var myLine = svg.append("svg:line")
        .attr('class', 'best-fit')
        .attr('id', 'hidden')
        .attr("x1", 40)
        .attr("y1", yScale(train[0]))
        .attr("x2", 830)
        .attr("y2", yScale(train[1]))
        .style("stroke", "white");
    }

